<!-- Galerie -->
<section class="gallery">

  {% for image in images %}
    <div class="gallery__item">
      <a class="gallery__item-link" href="{{ image.url }}" target="_blank" data-width="{{ image.width }}" data-height="{{ image.height }}" data-caption="">
        <img class="gallery__item-img" src="{{ image.url }}" alt="{{ image.name }}" {{ image.size }}>
      </a>
    </div>
  {% endfor %}

</section>
